    ui = true
[alias]
    chuck-norris = push -f
    cia = commit --amend
    dm = diff master
    fp = fetch -p
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    pr = pull --rebase
    sa = stash apply
    ski = stash --keep-index
    sp = stash pop
